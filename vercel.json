{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/node",
      "config": {
        "buildCommand": "npm run vercel-build"
      }
    }
  ],
  "routes": [
    {
      "src": "/assets/(.*)",
      "dest": "/dist/public/assets/$1"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|ico|svg|woff|woff2|ttf|eot|otf)$)",
      "dest": "/dist/public/$1"
    },
    {
      "src": "/api/(.*)",
      "dest": "/dist/vercel-adapter.mjs"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/vercel-adapter.mjs"
    }
  ]
}
