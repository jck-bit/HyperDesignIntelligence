{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static-build", "config": { "distDir": "dist/public" } }
  ],
  "routes": [
    { "src": "/api/(.*)", "dest": "http://ec2-13-60-196-19.eu-north-1.compute.amazonaws.com:3000/api/$1" },
    { "src": "/ws", "dest": "http://ec2-13-60-196-19.eu-north-1.compute.amazonaws.com:3000/ws" },
    { "handle": "filesystem" },
    { "src": "/(.*)", "dest": "/index.html" }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
